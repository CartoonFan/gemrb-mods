  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  See(NearestEnemyOf(Myself),0)
THEN
  RESPONSE #100
    MarkSpellAndObject("131123061418",LastMarkedObject,7)
      // CLERIC_RIGID_THINKING
      // WIZARD_HOLD_PERSON
      // CLERIC_POISON
    Continue()
END

