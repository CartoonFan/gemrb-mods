IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  Or(2)
    See([0.0.LIZARDMAN],0)
    See([0.0.YUANTI],0)
THEN
  RESPONSE #100
    MarkSpellAndObject("1115",Myself,7)
      // CLERIC_ARMOR_OF_FAITH
    MarkSpellAndObject("110113131210",LastMarkedObject,7)
      // CLERIC_BLESS
      // CLERIC_PRAYER
      // CLERIC_MINOR_ELEMENTAL_BARRIER
    Continue()
  RESPONSE #100
    MarkSpellAndObject("110113131210",LastMarkedObject,7)
      // CLERIC_BLESS
      // CLERIC_PRAYER
      // CLERIC_MINOR_ELEMENTAL_BARRIER
    MarkSpellAndObject("1115",Myself,7)
      // CLERIC_ARMOR_OF_FAITH
    Continue()
END

IF
  !IsMarkedSpell(MARKED_SPELL)
THEN
  RESPONSE #100
    Spell(SpellTarget,MARKED_SPELL)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
    ForceMarkedSpell(MARKED_SPELL)
END

IF
  HaveSpell(CLERIC_REMOVE_PARALYSIS)
  Or(2)
    See([0.0.LIZARDMAN],0)
    See([0.0.YUANTI],0)
  IsSpellTargetValid(LastMarkedObject,CLERIC_REMOVE_PARALYSIS,0)
  CheckSpellState(LastMarkedObject,HELD)
THEN
  RESPONSE #100
    Spell(LastMarkedObject,CLERIC_REMOVE_PARALYSIS)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
