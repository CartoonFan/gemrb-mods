IF
  CheckSpellState(Myself,SANCTUARY)
  See([GOODCUTOFF],0)
  !See([0.0.YUANTI],0)
  !See([0.0.LIZARDMAN],0)
  Or(2)
    See([0.0.YUANTI][0.0.10000.10000],0)
    See([0.0.LIZARDMAN][0.0.10000.10000],0)
THEN
  RESPONSE #100
    MoveToObjectUntilSee(LastMarkedObject)
END

IF
  CheckSpellState(Myself,SANCTUARY)
THEN
  RESPONSE #100
    RandomWalk()
END

IF
  HaveSpell(CLERIC_CURE_SERIOUS_WOUNDS)
  See([0.0.LIZARDMAN],0)
  HPLostGT(LastSeenBy(Myself),37)
  IsSpellTargetValid(LastSeenBy(Myself),CLERIC_CURE_SERIOUS_WOUNDS,0)
THEN
  RESPONSE #100
    Spell(LastSeenBy(Myself),CLERIC_CURE_SERIOUS_WOUNDS)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
END

