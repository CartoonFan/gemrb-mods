IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  Allegiance(Myself,ENEMY)
  See(NearestEnemyOf(Myself),0)
THEN
  RESPONSE #100
    MarkSpellAndObject("1901161613221608",LastMarkedObject,15)
      // CLERIC_ELEMENTAL_LEGION
      // CLERIC_DOLOROUS_DECAY
      // CLERIC_MOLD_TOUCH
      // CLERIC_SOLS_SEARING_ORB
    Continue()
END

IF
  !IsMarkedSpell(MARKED_SPELL)
THEN
  RESPONSE #100
    ForceSpell(SpellTarget,MARKED_SPELL)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
    ForceMarkedSpell(MARKED_SPELL)
END

IF
  HasItem("Destruc",Myself)  // ~Destruction~
  Or(2)
    See([ENEMY],0)
    See([0.0.YUANTI],0)
THEN
  RESPONSE #100
    AttackOneRound(LastMarkedObject)
END

IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  Or(2)
    See([ENEMY],0)
    See([0.0.YUANTI],0)
THEN
  RESPONSE #100
    MarkSpellAndObject("1717",Myself,15)
      // CLERIC_DESTRUCTION
    MarkSpellAndObject("161613221608",LastMarkedObject,15)
      // CLERIC_DOLOROUS_DECAY
      // CLERIC_MOLD_TOUCH
      // CLERIC_SOLS_SEARING_ORB
    Continue()
  RESPONSE #100
    MarkSpellAndObject("161613221608",LastMarkedObject,15)
      // CLERIC_DOLOROUS_DECAY
      // CLERIC_MOLD_TOUCH
      // CLERIC_SOLS_SEARING_ORB
    MarkSpellAndObject("1717",Myself,15)
      // CLERIC_DESTRUCTION
    Continue()
END

