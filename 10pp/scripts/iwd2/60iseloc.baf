IF
  Global("Move","LOCALS",1)
  NearLocation(Myself,1600,2416,5)
THEN
  RESPONSE #100
    SetGlobal("Move","LOCALS",0)
    Continue()
END

IF
  Global("Move","LOCALS",0)
  Range("HeartstoneGem",19,GREATER_THAN)  // HeartstoneGem
THEN
  RESPONSE #100
    SetGlobal("Move","LOCALS",1)
    MoveToPoint([1600.2416])
END

IF
  Global("6001_Fight","GLOBAL",1)
  Global("Buff","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Buff","LOCALS",1)
    ForceSpell(Myself,CLERIC_DIVINE_SHELL)
    SpellWait(CLERIC_DIVINE_SHELL)
    ForceSpell(Myself,CLERIC_IMPERVIOUS_SANCTITY_OF_MIND)
    SpellWait(CLERIC_IMPERVIOUS_SANCTITY_OF_MIND)
    ForceSpell(Myself,CLERIC_AURA_OF_VITALITY)
    SpellWait(CLERIC_AURA_OF_VITALITY)
END

IF
  See(Player1,0)
  IsSpellTargetValid(LastMarkedObject,CLERIC_HEAL,SPELLCAST_IGNORE_HAS_CHECK)
  Global("6001_Fight","GLOBAL",1)
  HPPercentLT(LastSeenBy,51)
  !Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    ForceSpell(LastMarkedObject,CLERIC_HEAL)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
END

