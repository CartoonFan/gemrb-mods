IF
  !NumberOfTimesTalkedTo(0)
THEN
  RESPONSE #100
    ChangeCurrentScript("")
END

IF
  NumberOfTimesTalkedTo(0)
  !Allegiance(Myself,ENEMY)
  !Dead(Myself)  // 
  See(Protagonist,0)
THEN
  RESPONSE #100
    StartCutSceneMode()
    ClearAllActions()
    SetDialogueRange(300)
    MultiPlayerSync()
    Dialogue(LastMarkedObject)
END

