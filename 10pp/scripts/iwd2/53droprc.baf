IF
  HaveSpell(INNATE_TURN_UNDEAD)
  NumCreatureGT([GOODCUTOFF.UNDEAD],0)
THEN
  RESPONSE #100
    ReallyForceSpell(Myself,INNATE_TURN_UNDEAD)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
END

IF
  !TimerActive(14)
  StateCheck(LastAttackerOf,STATE_INVISIBLE)
  StateCheck(LastAttackerOf,STATE_NONDETECTION)
  HaveSpell(CLERIC_INVISIBILITY_PURGE)
THEN
  RESPONSE #100
    StartTimer(14,21)
    Spell(LastAttackerOf,CLERIC_INVISIBILITY_PURGE)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
END

IF
  !TimerActive(14)
  Heard([ENEMY],0)
  HaveSpell(CLERIC_INVISIBILITY_PURGE)
THEN
  RESPONSE #100
    StartTimer(14,21)
    Spell(LastHeardBy,CLERIC_INVISIBILITY_PURGE)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
END

IF
  NumCreatureLT([ENEMY.0.ELF],6)
  HaveSpell(CLERIC_CLOUD_OF_PESTILENCE)
THEN
  RESPONSE #100
    Spell(FarthestEnemyOf(Myself),CLERIC_CLOUD_OF_PESTILENCE)
    SpellWait(CLERIC_CLOUD_OF_PESTILENCE)
END

IF
  HasItem("00potn04",Myself)  // ~Potion of Healing~
  HPLostGT(Myself,33)
THEN
