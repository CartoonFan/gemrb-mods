IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  !NumberOfTimesTalkedTo(0)
  SetLastMarkedObject(Myself)
THEN
  RESPONSE #100
    MarkSpellAndObject("140616091215",Myself,3)
      // CLERIC_DEFENSIVE_HARMONY
      // CLERIC_DIVINE_SHELL
      // CLERIC_DRAW_UPON_HOLY_MIGHT
