IF
  HasItem("BBoDisa",Myself)  // ~Black Blade of Disaster~
  See(NearestEnemyOf(Myself),0)
THEN
  RESPONSE #100
    AttackOneRound(LastMarkedObject)
END

IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  See(NearestEnemyOf(Myself),0)
THEN
  RESPONSE #100
    MarkSpellAndObject("2907",Myself,3)
      // WIZARD_BLACK_BLADE_OF_DISASTER
    MarkSpellAndObject("2910",LastMarkedObject,3)
      // WIZARD_EXECUTIONERS_EYES
    Continue()
END

IF
  !IsMarkedSpell(MARKED_SPELL)
THEN
  RESPONSE #100
    Spell(SpellTarget,MARKED_SPELL)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
    ForceMarkedSpell(MARKED_SPELL)
END

IF
  HasItem("BBoDisa",Myself)  // ~Black Blade of Disaster~
  HaveSpell(WIZARD_ANTIMAGIC_SHELL)
  SetLastMarkedObject(Myself)
  IsSpellTargetValid(LastMarkedObject,WIZARD_ANTIMAGIC_SHELL,0)
THEN
  RESPONSE #100
    Spell(LastMarkedObject,WIZARD_ANTIMAGIC_SHELL)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
END

IF
  See(NearestEnemyOf(Myself),0)
THEN
  RESPONSE #100
    EquipWeapon()
    AttackOneRound(LastMarkedObject)
END

IF
  !See(NearestEnemyOf(Myself),0)
THEN
  RESPONSE #100
    MoveToObjectUntilSee(NearestPC)
END

IF
THEN
  RESPONSE #100
    MoveToObject(NearestPC)
END

