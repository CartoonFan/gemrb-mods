IF
  Global("Pool_Tainted","GLOBAL",1)
  Global("TeleportIsairMadaeHP33","MYAREA",0)
  HPPercentGT(Myself,33)
THEN
  RESPONSE #100
    MatchHP("Isair2",CURRENT_HP)
    Continue()
END

IF
  Global("Obj0","LOCALS",0)
  See(NearestEnemyOf(Myself),0)
  IsSpellTargetValid(LastMarkedObject,WIZARD_SUMMON_MONSTER_III_ISAIR,SPELLCAST_IGNORE_HAS_CHECK)
THEN
  RESPONSE #100
    SetGlobal("Obj0","LOCALS",1)
    ForceSpell(LastMarkedObject,WIZARD_SUMMON_MONSTER_III_ISAIR)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
END

