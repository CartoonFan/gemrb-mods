IF
  GlobalTimerNotExpired("PrismFireDelay","MYAREA")
THEN
  RESPONSE #100
    NoAction()
END

IF
  Global("41PrismTarget","MYAREA",2)
  OpenState("AR4102_Door10",TRUE)  // AR4102_Door10
THEN
  RESPONSE #100
    TriggerActivation("ShadowTrap",FALSE)
    Unlock("AR4102_Door9")
    OpenDoor("AR4102_Door9")
    Continue()
END

