IF
  OpenState("AR4100_Rockwall",TRUE)  // AR4100_Rockwall
THEN
  RESPONSE #100
    ChangeCurrentScript("")
    TriggerActivation(Myself,FALSE)
END

IF
  OpenState("AR4100_Rockwall",FALSE)  // AR4100_Rockwall
  See([PC],0)
THEN
  RESPONSE #100
    SetGlobal("41DestroyBlockade","MYAREA",1)
    AddJournalEntry(22968)
    ChangeCurrentScript("")
    TriggerActivation(Myself,FALSE)
END

