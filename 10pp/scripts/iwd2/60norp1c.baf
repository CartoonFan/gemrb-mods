IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  Global("I_Have_Seen_An_Enemy","LOCALS",1)
  !See(NearestEnemyOf(Myself),0)
  Or(9)
    See("Neo_Orog1",0)  // Neo_Orog1
    See("Neo_Orog2",0)  // Neo_Orog2
    See("Neo_Orog3",0)  // Neo_Orog3
    See("Neo_Orog4",0)  // Neo_Orog4
    See("Neo_Orog5",0)  // Neo_Orog5
    See("Neo_Orog6",0)  // Neo_Orog6
    See("Neo_Orog7",0)  // Neo_Orog7
    See("Neo_Orog8",0)  // Neo_Orog8
    SetLastMarkedObject(Myself)
THEN
  RESPONSE #100
    MarkSpellAndObject("121012191107",LastMarkedObject,7)
      // CLERIC_MINOR_ELEMENTAL_BARRIER
      // CLERIC_SPELL_SHIELD
      // CLERIC_PROTECTION_FROM_EVIL
    Continue()
END

