IF
  !IsMarkedSpell(MARKED_SPELL)
THEN
  RESPONSE #100
    Spell(SpellTarget,MARKED_SPELL)
    WaitAnimation(Myself,WALK)
    WaitAnimation(Myself,CONJURE)
    WaitAnimation(Myself,CAST)
    ForceMarkedSpell(MARKED_SPELL)
END

IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  SetLastMarkedObject(Myself)
THEN
  RESPONSE #100
    MarkSpellAndObject("1201","Neo_Orog1",0)
      // CLERIC_AID
    MarkSpellAndObject("110114141406",Myself,3)
      // CLERIC_BLESS
      // CLERIC_RECITATION
      // CLERIC_DEFENSIVE_HARMONY
    MarkSpellAndObject("1503",NearestEnemyOf(Myself),0)
      // CLERIC_FLAME_STRIKE
    Continue()
END

