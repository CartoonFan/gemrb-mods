IF
  ForceMarkedSpell(MARKED_SPELL)
  SetSpellTarget(Nothing)
  HPPercentLT(Myself,50)
THEN
  RESPONSE #100
    MarkSpellAndObject("11031502",Myself,SPELLCAST_IGNORE_SEE)
      // CLERIC_CURE_LIGHT_WOUNDS
      // CLERIC_CURE_CRITICAL_WOUNDS
    Continue()
END

IF
  IsMarkedSpell(MARKED_SPELL)
  HPPercentLT("Neo_Orog1",50)  // Neo_Orog1
  See("Neo_Orog1",0)  // Neo_Orog1
THEN
  RESPONSE #100
    MarkSpellAndObject("11031502",LastSeenBy(Myself),SPELLCAST_IGNORE_SEE)
      // CLERIC_CURE_LIGHT_WOUNDS
      // CLERIC_CURE_CRITICAL_WOUNDS
    Continue()
END

