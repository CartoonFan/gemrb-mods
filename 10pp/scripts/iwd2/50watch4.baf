IF
  Global("TEAM_10","MYAREA",0)
  OpenState("AR5004_1A",TRUE)  // AR5004_1A
  OpenState("AR5004_2A",TRUE)  // AR5004_2A
  OpenState("AR5004_3A",TRUE)  // AR5004_3A
  OpenState("AR5004_4A",TRUE)  // AR5004_4A
THEN
  RESPONSE #100
    SetInterrupt(FALSE)
    PlaySound("am1000j")
