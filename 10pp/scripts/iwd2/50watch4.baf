    SetGlobal("TEAM_9","MYAREA",1)
    SetGlobal("TEAM_10","MYAREA",1)
    OpenDoor("AR5004_TeleportBlock")
    AddXPVar("LEVEL_9_AVERAGE",26667)  // ~You've managed to open the barbarians' Gate-of-Stone.~
    StartCutScene("50cRokFA")
    SetInterrupt(TRUE)
END

