IF
  NearLocation(Myself,[1086.1803],20)
THEN
  RESPONSE #100
    RunToSavedLocation()
END

IF
  See([PC])
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack([PC])
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack(LastAttackerOf(Myself))
END

IF
  Help([0.0.0.0.GRONK])
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack(Protagonist)
END

