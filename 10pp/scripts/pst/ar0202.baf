IF
  Vacant()
  Global("Vaxis_Leave","GLOBAL",1)
THEN
  RESPONSE #100
    GlobalSet("0202_Left","AR0202",1)
END

IF
  !Exists([ENEMY][0.0.10000.10000])
  Global("Mortuary_Alert_2","GLOBAL",1)
THEN
  RESPONSE #100
    GlobalSet("Mortuary_Alert_2","GLOBAL",0)
    Continue()
END

IF
  Global("0202_DoorA_Opened","GLOBAL",1)
THEN
