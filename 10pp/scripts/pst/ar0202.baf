IF
  !Global("Current_Area","GLOBAL",202)
  Global("Vaxis_Leave","GLOBAL",1)
  Global("0202_Left","AR0202",0)
THEN
  RESPONSE #100
    GlobalSetGlobal("Previous_Area","GLOBAL","Current_Area","GLOBAL")
    GlobalSet("Current_Area","GLOBAL",202)
    GlobalSet("0202_Left","AR0202",1)
    Continue()
END

IF
  !Global("Current_Area","GLOBAL",202)
THEN
  RESPONSE #100
    GlobalSetGlobal("Previous_Area","GLOBAL","Current_Area","GLOBAL")
    GlobalSet("Current_Area","GLOBAL",202)
    Continue()
END

