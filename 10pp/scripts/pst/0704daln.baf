IF
  WasInDialog()
THEN
  RESPONSE #100
    ReturnToSavedPlace()
    Continue()
END

IF
  !NearSavedLocation(0)
THEN
  RESPONSE #100
    ReturnToSavedPlace()
END

IF
  True()
THEN
  RESPONSE #100
    WaitRandom(5,10)
    FloatMessage(Myself,68000)  // ~::Whisper::~
  RESPONSE #100
    WaitRandom(5,10)
    FloatMessage(Myself,68001)  // ~::Kiss::~
END

