IF
  !Global("Current_Area","GLOBAL",604)
THEN
  RESPONSE #100
    GlobalSet("Current_Area","GLOBAL",604)
    Continue()
END

IF
  Exists("Nameless")  // ~Nameless One~
  !GlobalGT("AR0604_Visited","GLOBAL",0)
THEN
  RESPONSE #100
    GlobalSet("AR0604_Visited","GLOBAL",1)
    Continue()
END

IF
  GlobalTimerExpired("Sen4","AR0604")
  BitCheck("0604_Status","AR0604",BIT2)
THEN
  RESPONSE #100
    BitClear("0604_Status","AR0604",BIT2)
    CreateCreature("SensM3",[274.1116])  // ~Sensate~
END

