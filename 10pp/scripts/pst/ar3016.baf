IF
  !Global("Current_Area","GLOBAL",3016)
THEN
  RESPONSE #100
    GlobalSet("Current_Area","GLOBAL",3016)
    Continue()
END

IF
  Global("Mob_Dead","AR3016",3)
  !Dead("Distiller")  // Distiller
THEN
  RESPONSE #100
    IncrementGlobal("Curst_Counter","GLOBAL",1)
    GlobalSet("Mob_Dead","AR3016",4)
END

