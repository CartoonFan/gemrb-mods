IF
  Global("Team_7_Leader","GLOBAL",1)
THEN
  RESPONSE #100
    ChangeAIScript("1600t7gl",DEFAULT)
END

IF
  Specifics(Myself,LEAVING)
  NearLocation(Myself,[3810.1830],0)
THEN
  RESPONSE #100
    DestroySelf()
END

IF
  !Specifics(Myself,LEAVING)
THEN
  RESPONSE #100
    ChangeSpecifics(Myself,LEAVING)
    MoveToPoint([3810.1830])
END

