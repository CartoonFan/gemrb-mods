IF
  HPPercentLT(Myself,50)
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    Attack(NearestEnemyOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack(LastAttackerOf(Myself))
END

IF
  True()
THEN
  RESPONSE #100
    WaitRandom(20,30)
    Face(2)
    MoveToPoint([2092.1817])
    MoveToPoint([1318.1817])
    MoveToPoint([1013.1510])
    MoveToPoint([1013.1316])
    MoveToPoint([1115.1213])
    MoveToPoint([1115.1061])
    MoveToPoint([1665.512])
    MoveToPoint([1665.175])
    MoveToPoint([1772.67])
    MoveToPoint([1772.0])
    ChangeAIScript("0400tm4a",GENERAL)
END

