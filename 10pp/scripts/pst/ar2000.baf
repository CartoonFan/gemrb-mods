IF
  !Global("Current_Area","GLOBAL",2000)
THEN
  RESPONSE #100
    GlobalSet("Current_Area","GLOBAL",2000)
    GlobalSet("Destroy_Cattys","AR2000",1)
    ChangeAIScript("ar2000ch",DEFAULT)
    Continue()
END

IF
  Global("Destroy_Cattys","AR2000",1)
THEN
  RESPONSE #100
    GlobalSet("Destroy_Cattys","AR2000",0)
END

