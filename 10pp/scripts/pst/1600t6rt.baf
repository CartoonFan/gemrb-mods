IF
  GlobalTimerExpired("Leaving_6","GLOBAL")
THEN
  RESPONSE #100
    ChangeAIScript("1600t6rn",DEFAULT)
END

IF
  Range([PC],3)
THEN
  RESPONSE #100
    Attack([PC])
END

IF
  See(NearestEnemyOf(Myself))
  Global("Team_6_Attack","AR1600",1)
THEN
  RESPONSE #100
    Help()
    Attack(NearestEnemyOf(Myself))
END

IF
  True()
THEN
  RESPONSE #100
    FaceObject([PC])
END

