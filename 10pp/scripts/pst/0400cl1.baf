IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
END

IF
  True()
THEN
  RESPONSE #100
    Face(0)
    WaitRandom(5,9)
    MoveToPoint([2007.940])
    MoveToPoint([1858.792])
    MoveToPoint([1187.792])
    MoveToPoint([815.417])
    Face(6)
    WaitRandom(5,9)
END

