IF
  HPPercentLT(Myself,50)
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Attack(NearestEnemyOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Attack(LastAttackerOf(Myself))
END

IF
  True()
THEN
  RESPONSE #100
    WaitRandom(30,40)
    MoveToPoint([949.575])
    MoveToPoint([726.796])
    MoveToPoint([726.2136])
    MoveToPoint([1030.2441])
    MoveToPoint([1463.2441])
    MoveToPoint([1549.2353])
    MoveToPoint([1716.2353])
    MoveToPoint([1872.2197])
    MoveToPoint([2288.2098])
    MoveToPoint([2436.2098])
    MoveToPoint([2652.2314])
    MoveToPoint([3199.2314])
    Face(4)
    ChangeAIScript("0300tm2a",GENERAL)
END

