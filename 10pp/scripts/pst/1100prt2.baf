IF
  HarmlessEntered("Nordom")  // ~Modron~
THEN
  RESPONSE #100
    VerbalConstant("Nordom",NORDOM_PORTAL)
    Continue()
END

IF
  HarmlessEntered([PC])
  Global("Curst_Portal","GLOBAL",1)
THEN
  RESPONSE #100
    ActivatePortalCursor("to0800",1)
    EnablePortalTravel("to0800",1)
    Wait(15)
    ActivatePortalCursor("to0800",0)
    EnablePortalTravel("to0800",0)
END

