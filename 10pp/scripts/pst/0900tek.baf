IF
  Internal(Myself,INTERNAL_0,2)
THEN
  RESPONSE #100
    MoveToPoint([1401.2329])
    Face(0)
    StartRandomTimer(1,5,10)
    IncInternal(Myself,INTERNAL_0,1)
END

