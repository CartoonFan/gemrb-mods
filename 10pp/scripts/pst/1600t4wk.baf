IF
  OnCreation()
THEN
  RESPONSE #100
    SetInternal(Myself,INTERNAL_1,0)
    ChangeSpecifics(Myself,TEAM_4_LEADER)
    ChangeEnemyAlly(Myself,NEUTRAL)
END

IF
  Die()
THEN
  RESPONSE #100
    IncrementGlobal("RatWarren","GLOBAL",1)
    GlobalSet("Team_4_Leader","GLOBAL",0)
    GlobalSet("1600_PC_Enemy","AR1600",1)
END

IF
  Global("1600_PC_Enemy","AR1600",1)
  Allegiance(Myself,NEUTRAL)
THEN
  RESPONSE #100
    Enemy()
END

IF
  Global("MAO_Warn","GLOBAL",1)
  Allegiance(Myself,NEUTRAL)
THEN
  RESPONSE #100
    Enemy()
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    GlobalSet("Team_4_Leader","GLOBAL",1)
    ChangeAIScript("1600t4ld",DEFAULT)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Help()
    GlobalSet("1600_PC_Enemy","AR1600",1)
    GlobalSet("Team_4_Leader","GLOBAL",1)
    ChangeAIScript("1600t4ld",DEFAULT)
END

