IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Help()
    Enemy()
END

IF
  True()
THEN
  RESPONSE #100
    Wait(5)
    MoveToPoint([845.418])
    Face(4)
    Wait(10)
    MoveToPoint([1094.459])
    Face(12)
    Continue()
END

