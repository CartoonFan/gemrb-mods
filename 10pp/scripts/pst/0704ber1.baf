IF
  OnCreation()
THEN
  RESPONSE #100
    ChangeTeam(Myself,TEAM_3)
END

IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Help()
END

IF
  See([PC])
THEN
  RESPONSE #100
    Wait(1)
    RunToPoint([1913.1859])
    Face(4)
    ChangeAIScript("0704ber2",DEFAULT)
END

IF
  !NearSavedLocation(0)
THEN
  RESPONSE #100
    ReturnToSavedPlace()
END

