IF
  See(NearestEnemyOf(Myself))
  Global("Rat_Leader_Done","AR0108",1)
THEN
  RESPONSE #100
    Attack(NearestEnemyOf(Myself))
END

IF
  Range([PC],3)
THEN
  RESPONSE #100
    Attack([PC])
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    RunToPoint([4300.1750])
    FaceObject([PC])
END

