IF
  Die()
THEN
  RESPONSE #100
    Help()
END

IF
  OnCreation()
THEN
  RESPONSE #100
    Enemy()
    ChangeSpecifics(Myself,TW_CAMP_2)
END

IF
  HPPercentLT(Myself,10)
THEN
  RESPONSE #100
    RunAwayFromEx(LastAttackerOf(Myself),50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Help()
    SetInternal(Myself,INTERNAL_0,1)
    Attack(NearestEnemyOf(Myself))
END

IF
  Help([0.0.0.0.0.0.TW_CAMP_2])
THEN
  RESPONSE #100
    Enemy()
    Help()
    SetInternal(Myself,INTERNAL_0,1)
    Attack(LastAttackerOf(LastHelp))
END

IF
  LOS([PC],20)
  Internal(Myself,INTERNAL_0,0)
THEN
  RESPONSE #100
    RunToPoint([3135.1221])
    Face(2)
    SetInternal(Myself,INTERNAL_0,1)
END

IF
  See([PC])
  Internal(Myself,INTERNAL_0,1)
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack([PC])
END

