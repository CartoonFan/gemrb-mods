IF
  Vacant()
  Global("Pregnant_Quest","GLOBAL",4)
THEN
  RESPONSE #100
    GlobalSet("Alley_Door","AR0405",1)
    GlobalSet("Pregnant_Quest","GLOBAL",5)
END

IF
  Global("Alley_Door","AR0405",1)
THEN
  RESPONSE #100
    OpenDoor("AR0405D1")
    GlobalSet("Alley_Door","AR0405",0)
END

IF
  Global("AR0405_Visited","GLOBAL",0)
  InParty("Annah")  // ~Tiefling~
THEN
  RESPONSE #100
    StartCutSceneMode()
    StartCutScene("0405cut1")
    GlobalSet("AR0405_Visited","GLOBAL",1)
END

IF
  Global("AR0405_Visited","GLOBAL",0)
THEN
  RESPONSE #100
    StartCutSceneMode()
    StartCutScene("0405cut2")
    GlobalSet("AR0405_Visited","GLOBAL",1)
END

IF
  !Global("Current_Area","GLOBAL",405)
  GlobalGT("Pregnant_Quest","GLOBAL",0)
  GlobalLT("Pregnant_Quest","GLOBAL",3)
THEN
  RESPONSE #100
    SinisterPoof("alley_s",[855.375],-1)
    GlobalSet("Current_Area","GLOBAL",405)
END

IF
  Global("Pregnant_Quest","GLOBAL",3)
THEN
  RESPONSE #100
    GlobalSet("Pregnant_Quest","GLOBAL",4)
    TeleportParty("AR0500",[543.1953],14)
END

IF
  !Global("Current_Area","GLOBAL",405)
THEN
  RESPONSE #100
