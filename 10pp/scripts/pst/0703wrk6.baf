IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Help()
    Enemy()
END

IF
  True()
THEN
  RESPONSE #100
    Wait(13)
    MoveToPoint([369.701])
    Face(2)
    Wait(10)
    MoveToPoint([784.792])
    Face(6)
    Continue()
END

