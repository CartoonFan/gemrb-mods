IF
  WasInDialog()
THEN
  RESPONSE #100
    ReturnToSavedPlace()
    Continue()
END

IF
  !NearSavedLocation(0)
THEN
  RESPONSE #100
    ReturnToSavedPlace()
END

IF
  True()
THEN
  RESPONSE #100
    Wait(20)
    PlaySequence(ANIM_TALK1)
    FloatMessage(Myself,68004)  // ~"Are you ready, yet?"~
END

