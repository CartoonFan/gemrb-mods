IF
  Vacant()
THEN
  RESPONSE #100
    GlobalSet("No_Cube_Here","GLOBAL",0)
    GlobalSet("Trelon_Team_1","AR0708",0)
    GlobalSet("Trelon_Team_2","AR0708",0)
END

IF
  !Global("Current_Area","GLOBAL",708)
  Exists("Nameless")  // ~Nameless One~
THEN
  RESPONSE #100
    GlobalSet("Current_Area","GLOBAL",708)
    GlobalSet("No_Cube_Here","GLOBAL",1)
    Continue()
END

IF
  !GlobalGT("AR0708_Visited","GLOBAL",0)
  Exists("Nameless")  // ~Nameless One~
THEN
  RESPONSE #100
    GlobalSet("AR0708_Visited","GLOBAL",1)
    Continue()
END

