IF
  OnCreation()
THEN
  RESPONSE #100
    ChangeSpecifics(Myself,PERSON_3)
    Continue()
END

IF
  WasInDialog()
THEN
  RESPONSE #100
    ReturnToSavedPlace()
    Continue()
END

IF
  Global("Execution_Chaos","AR0900",1)
THEN
  RESPONSE #100
    Enemy()
    Attack(LastAttackerOf(Myself))
END

IF
  Global("Curst_Execution_One","GLOBAL",16)
THEN
  RESPONSE #100
    PlaySequence(ANIM_STANDTOSTANCE)
    GlobalSet("Curst_Execution_One","GLOBAL",0)
    PlaySequence(ANIM_ATTACK1)
END

