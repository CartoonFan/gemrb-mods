IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
END

IF
  True()
THEN
  RESPONSE #100
    Face(4)
    Wait(3)
    MoveToPoint([3171.2076])
    MoveToPoint([3436.2076])
    Face(8)
    Wait(5)
    MoveToPoint([3171.2076])
    MoveToPoint([3070.1975])
END

