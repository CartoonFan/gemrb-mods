IF
  HPPercentLT(Myself,50)
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    Attack(NearestEnemyOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack(LastAttackerOf(Myself))
END

IF
  Help([0.0.DUSTMAN])
THEN
  RESPONSE #100
    Enemy()
    Attack(LastAttackerOf(LastHelp))
END

IF
  True()
THEN
  RESPONSE #100
    Wait(5)
    Face(0)
    Wait(5)
    Face(2)
    Wait(5)
    Face(0)
    Wait(1)
    Face(2)
    MoveToPoint([2127.1015])
    Face(0)
    Wait(5)
    Face(14)
    Wait(1)
    Face(2)
    Wait(1)
    Face(0)
    Wait(1)
    MoveToPoint([2366.1526])
    Face(14)
END

