IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Attack(NearestEnemyOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Attack(NearestEnemyOf(Myself))
END

IF
  !NearLocation(Myself,[1325.651],0)
THEN
  RESPONSE #100
    MoveToPoint([1325.651])
    Face(14)
END

IF
  See([PC])
THEN
  RESPONSE #100
    FaceObject([PC])
END

