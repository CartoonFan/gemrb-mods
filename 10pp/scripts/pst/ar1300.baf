IF
  Vacant()
THEN
  RESPONSE #100
    GlobalSet("No_Cube_Here","GLOBAL",0)
END

IF
  Global("No_Cube_Here","GLOBAL",0)
THEN
  RESPONSE #100
    GlobalSet("No_Cube_Here","GLOBAL",1)
    Continue()
END

IF
  !Global("Current_Area","GLOBAL",1300)
THEN
  RESPONSE #100
    GlobalSet("Current_Area","GLOBAL",1300)
    Continue()
END

IF
  Exists("Nameless")  // ~Nameless One~
  !GlobalGT("AR1300_Visited","GLOBAL",0)
THEN
  RESPONSE #100
    GlobalSet("AR1300_Visited","GLOBAL",1)
    Continue()
END

