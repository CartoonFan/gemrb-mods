IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    RunToPoint([460.262])
    DestroySelf()
END

IF
  AttackedBy([ANYONE],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    RunToPoint([460.262])
    DestroySelf()
END

IF
  True()
THEN
  RESPONSE #100
    FloatMessage(Myself,48389)  // ~"It's got to be here somewhere."~
    Wait(1)
    MoveToPoint([282.417])
    Face(14)
    MoveToPoint([323.291])
    FloatMessage(Myself,48671)  // ~"Where did that idiot hide it?"~
    Face(8)
    Wait(1)
    MoveToPoint([196.337])
    Face(2)
END

