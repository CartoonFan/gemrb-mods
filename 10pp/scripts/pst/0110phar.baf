IF
  Global("AR0403_Visited","GLOBAL",1)
THEN
  RESPONSE #100
    JumpToPoint([2186.838])
    Kill(Myself)
END

IF
  Die()
  Dead("Annah")  // ~Tiefling~
  GlobalLT("Know_Painted_Door","GLOBAL",2)
THEN
  RESPONSE #100
    GlobalSet("Game_Over","GLOBAL",3)
END

IF
  Allegiance(Myself,ENEMY)
  Dead("Annah")  // ~Tiefling~
  GlobalLT("Know_Painted_Door","GLOBAL",2)
THEN
  RESPONSE #100
    GlobalSet("Game_Over","GLOBAL",3)
END

IF
  Allegiance(Myself,ENEMY)
  GlobalLT("Pharod_Quest","GLOBAL",2)
THEN
  RESPONSE #100
    GlobalSet("Game_Over","GLOBAL",3)
END

IF
  Allegiance(Myself,ENEMY)
  GlobalGT("Pharod_Quest","GLOBAL",1)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,TRUE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    GlobalSet("BV_Enemy","GLOBAL",1)
    Enemy()
END

IF
  Global("Pharod_Talk","GLOBAL",1)
THEN
  RESPONSE #33
    WaitRandom(3,5)
    FloatMessage(Myself,19237)  // ~"Aye...what am I gonna do with Annah?"~
    GlobalSet("Pharod_Talk","GLOBAL",0)
  RESPONSE #33
    WaitRandom(3,5)
    FloatMessage(Myself,19236)  // ~"The Dusties be paying a pretty piece of jink for this delivery."~
    GlobalSet("Pharod_Talk","GLOBAL",0)
  RESPONSE #33
    WaitRandom(3,5)
    FloatMessage(Myself,19235)  // ~"Gotta get more bodies from the catacombs."~
    GlobalSet("Pharod_Talk","GLOBAL",0)
  RESPONSE #901
    WaitRandom(3,5)
    GlobalSet("Pharod_Talk","GLOBAL",0)
END

