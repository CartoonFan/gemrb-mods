IF
  Dead("Vrischika")  // Vrischika
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack(NearestEnemyOf(Myself))
END

IF
  InternalGT("Vrischika",INTERNAL_1,0)  // Vrischika
  InternalLT("Vrischika",INTERNAL_1,4)  // Vrischika
THEN
  RESPONSE #100
