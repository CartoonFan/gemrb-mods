IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Help()
END

IF
  !NearLocation(Myself,[1756.948],10)
THEN
  RESPONSE #100
    MoveToPoint([1756.948])
END

