IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
END

IF
  True()
THEN
  RESPONSE #100
    Face(6)
    WaitRandom(6,12)
    MoveToPoint([1424.1050])
    Face(6)
    WaitRandom(6,12)
    MoveToPoint([1630.1050])
END

