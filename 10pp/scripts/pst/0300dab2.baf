IF
  HPPercentLT(Myself,50)
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    Attack(NearestEnemyOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    GlobalSet("Enemy_Dabus","GLOBAL",1)
    IncrementGlobal("Lady","GLOBAL",5)
    Enemy()
    Help()
    Attack(LastAttackerOf(Myself))
END

IF
  Help([0.0.0.0.DABUS])
THEN
  RESPONSE #100
    Enemy()
    Help()
    Attack(NearestEnemyOf(LastHelp))
END

IF
  True()
THEN
  RESPONSE #100
    PlaySequence(ANIM_MISC1)
    PlaySequence(ANIM_MISC1)
    PlaySequence(ANIM_MISC1)
    MoveToPoint([585.776])
    Face(8)
    PlaySequence(ANIM_MISC1)
    PlaySequence(ANIM_MISC1)
    PlaySequence(ANIM_MISC1)
    MoveToPoint([484.776])
    Face(10)
END

