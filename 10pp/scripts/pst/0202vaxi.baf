IF
  Global("0202_Left","AR0202",1)
THEN
  RESPONSE #100
    DestroySelf()
END

IF
  Allegiance(Myself,ENEMY)
  Global("Mortuary_Alert_2","GLOBAL",1)
THEN
  RESPONSE #100
    ChangeEnemyAlly(Myself,NEUTRAL)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    RunAwayFromEx(LastAttackerOf(Myself),100,FALSE)
END

