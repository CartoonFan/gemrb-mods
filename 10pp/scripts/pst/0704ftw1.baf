IF
  WasInDialog()
THEN
  RESPONSE #100
    ReturnToSavedPlace()
    Continue()
END

IF
  !NearSavedLocation(0)
THEN
  RESPONSE #100
    ReturnToSavedPlace()
END

IF
  True()
THEN
  RESPONSE #100
    Wait(25)
    PlaySequence(ANIM_TALK1)
    FloatMessage(Myself,68005)  // ~"Almost... almost..."~
END

