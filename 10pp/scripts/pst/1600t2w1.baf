IF
  See([PC])
THEN
  RESPONSE #100
    Help()
    Help()
    Attack([PC])
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Help()
    Attack(LastAttackerOf(Myself))
END

IF
  Help([0.0.TEAM_2])
THEN
  RESPONSE #100
    Attack(LastAttackerOf(LastHelp))
END

