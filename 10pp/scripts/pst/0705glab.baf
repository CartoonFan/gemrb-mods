IF
  Die()
THEN
  RESPONSE #100
    GlobalSet("Free_Agril","GLOBAL",3)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Enemy()
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Attack(LastAttackerOf(Myself))
END

IF
  !NearSavedLocation(0)
THEN
  RESPONSE #100
    ReturnToSavedPlace()
END

