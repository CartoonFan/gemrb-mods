IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    RunAwayFromEx([PC],50,FALSE)
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
END

IF
  True()
THEN
  RESPONSE #100
    Face(8)
    WaitRandom(6,12)
    MoveToPoint([1698.1097])
    Face(2)
    WaitRandom(6,12)
    MoveToPoint([1757.1041])
END

