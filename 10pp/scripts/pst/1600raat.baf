IF
  !InParty("Annah")  // ~Tiefling~
  Global("1600_PC_Enemy","AR1600",1)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Attack(LastAttackerOf(Myself))
END

IF
  Internal(Myself,INTERNAL_1,1)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Attack(LastAttackerOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Attack(LastAttackerOf(Myself))
END

