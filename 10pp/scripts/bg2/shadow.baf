IF
  Allegiance(Myself,ENEMY)
  !Global("HusamMove","GLOBAL",100)
THEN
  RESPONSE #100
    SetGlobal("HusamMove","GLOBAL",100)
END

IF
  Died(Myself)
  !Global("HusamMove","GLOBAL",100)
THEN
  RESPONSE #100
    SetGlobal("HusamMove","GLOBAL",100)
END

