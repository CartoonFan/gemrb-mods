IF
  Contains("MISC2A",Myself)
THEN
  RESPONSE #100
    DestroyItem("MISC2A")  // ~Doppelganger Wardstone~
    GiveItemCreate("MISC2A",[PC],0,0,0)  // ~Doppelganger Wardstone~
    ReallyForceSpell(Myself,TRAP_FIREBALL)
END

