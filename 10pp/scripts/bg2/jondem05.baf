IF
  Global("JonDemonTeleport","AR2906",1)
  Global("Teleport","LOCALS",0)
THEN
  RESPONSE #100
    SetInterrupt(FALSE)
    IncrementGlobal("Teleport","LOCALS",1)
    CreateVisualEffectObject("SPFLSRIN",Myself)
    Wait(1)
    CreateVisualEffectObject("SPFIREPI",Myself)
    JumpToPoint([3480.2155])
    Face(6)
    SetInterrupt(TRUE)
END

