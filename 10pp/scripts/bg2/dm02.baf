IF
  Clicked([ANYONE])
  Range(LastTrigger,10)
  GlobalTimerExpired("DM02Timer","GLOBAL")
  Global("DM02Active","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobalTimer("DM02Timer","GLOBAL",20)
    ForceSpell(LastTrigger,DM_FIRE_PROTECTION)
    TriggerActivation("DM02",FALSE)
    SetGlobal("DM02Active","GLOBAL",1)
END

