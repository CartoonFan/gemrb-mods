IF
  Opened([ANYONE])
  See([NOTGOOD])
  !StateCheck(LastTrigger,STATE_INVISIBLE)
  !StateCheck(LastTrigger,STATE_IMPROVEDINVISIBILITY)
THEN
  RESPONSE #100
    DisplayString(LastTrigger,9896)  // ~Someone has noticed you! You hear the guards being summoned!~ [AMB_D05A]
    Wait(3)
    CreateCreature("HALFEN",[-1.-1],0)  // HALFEN
    CreateCreature("HALFEN2",[-1.-1],0)  // HALFEN2
END

