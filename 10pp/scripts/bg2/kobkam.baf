IF
  HPLT(Myself,10)
THEN
  RESPONSE #50
    ReallyForceSpell(Myself,KAMIKAZE_EXPLOSION)
END

IF
  See(NearestEnemyOf(Myself))
  !Range(NearestEnemyOf(Myself),2)
THEN
  RESPONSE #100
    MoveToObject(NearestEnemyOf(Myself))
END

