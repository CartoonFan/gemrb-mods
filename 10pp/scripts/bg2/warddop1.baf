IF
  Clicked([ANYONE])
  OpenState("DOOR01",FALSE)  // DOOR01
  Range(LastTrigger,9)
THEN
  RESPONSE #100
    ActionOverride(LastTrigger,MoveToPoint([966.1328]))
    OpenDoor("DOOR01")
    CloseDoor("DOOR02")
    PlaySound("AMB_D04A")
END

IF
  Clicked([ANYONE])
  !Range(LastTrigger,9)
THEN
  RESPONSE #100
    DisplayString(Myself,23565)  // ~You are too far away to operate that device.~
END

