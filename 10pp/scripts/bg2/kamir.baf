IF
  Global("KamirWard","GLOBAL",1)
  !Range("Arenthis",4)  // ~Arenthis~
THEN
  RESPONSE #100
    MoveToObjectNoInterrupt("Arenthis")
END

IF
  Global("KamirWard","GLOBAL",1)
  Range("Arenthis",4)  // ~Arenthis~
  Range([PC],20)
THEN
  RESPONSE #100
    SetGlobal("KamirWard","GLOBAL",2)
    StartDialogNoSet([PC])
END

IF
  Global("KamirWard","GLOBAL",3)
THEN
  RESPONSE #100
    MoveToPointNoInterrupt([1805.2103])
    DestroySelf()
END

