IF
  Clicked([ANYONE])
  Range(LastTrigger,10)
  GlobalTimerExpired("DM01Timer","GLOBAL")
  Global("DM01Active","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobalTimer("DM01TImer","GLOBAL",20)
    ForceSpell(LastTrigger,DM_IMPROVED_HASTE)
    TriggerActivation("DM01",FALSE)
    SetGlobal("DM01Active","GLOBAL",1)
END

