THEN
  RESPONSE #100
    CutSceneId("cuwizc")  // 
    Wait(3)
    MoveToPointNoInterrupt([886.794])
    Face(4)
    Wait(1)
    DisplayStringWait(Myself,6300)  // ~These are the prisoners from the disturbance at Waukeen's Promenade.~ [CSCOW701]
    SmallWait(6)
    DisplayStringWait("cuwizsu",56379)  // ~What is known?~ [CSCOWS01]
    SmallWait(4)
    DisplayStringWait(Myself,56380)  // ~Naught but their names. The mage is Jon Irenicus. The girl is Imoen. ~ [CSCOW706]
    DisplayStringWait("cuimoen",56381)  // ~I didn't do anything; he did it all, I had...~ [CSIMO09]
    DisplayStringWait("cujon",38738)  // ~Silence, child. Allow the fool to make his judgement. ~ [DPJON146]
    DisplayStringWait("cuwizsu",56382)  // ~Why was this man not gagged? Did he not slay four of you?~ [CSCOWS04]
    SmallWait(6)
    DisplayStringWait(Myself,38752)  // ~We dared not approach. ~ [CSCOW707]
    SmallWait(6)
    DisplayStringWait("cuwiz4",38782)  // ~Regardless, in the end he came willingly.~ [CSCOW303]
    SmallWait(6)
    DisplayStringWait("cuwiz2",56383)  // ~What should be done with them sir?~ [CSCOW705]
    SmallWait(6)
    DisplayStringWait("cuwizsu",56605)  // ~They are deviants. Let them rot in Spellhold.~ [CSCOWS03]
    Wait(2)
    ActionOverride("cuwiz2",ForceSpell("cuimoen",FLASHY_1))
    ActionOverride("cuwiz4",ForceSpell("cujon",FLASHY_1))
    Wait(2)
    ActionOverride("cuimoen",DestroyItem("GLOBRING"))  // ~Ring~
    CreateVisualEffectObject("SPDISPMA","cuimoen")
    ActionOverride("cujon",DestroyItem("GLOBRING"))  // ~Ring~
    CreateVisualEffectObject("SPDISPMA","cujon")
    ActionOverride("cuwiz2",MoveToObjectNoInterrupt("cuimoen"))
    ActionOverride("cuwiz4",MoveToObjectNoInterrupt("cujon"))
    Wait(1)
    ActionOverride("cuwiz5",DestroySelf())
    ActionOverride("cuwiz2",ForceSpell("cuimoen",DRYAD_TELEPORT))
    ActionOverride("cuwiz4",ForceSpell("cujon",DRYAD_TELEPORT))
    ActionOverride("cuwiz1",ForceSpell(Myself,DRYAD_TELEPORT))
    ActionOverride("cuwiz3",ForceSpell(Myself,DRYAD_TELEPORT))
    Wait(1)
    ActionOverride("cuwiz2",Wait(1))
    ActionOverride("cuwiz4",Wait(1))
    ActionOverride("cuwiz2",DestroySelf())
    ActionOverride("cuwiz4",DestroySelf())
    Wait(3)
    StartCutScene("Movie01c")
END

