IF
  Global("ReturnCernd","LOCALS",0)
  OR(2)
    Dead("cefald01")  // ~Faldorn~
    Dead("cefald02")  // ~Faldorn~
THEN
  RESPONSE #100
    ReallyForceSpell(Myself,CERND_CERND)
    SetGlobal("ReturnCernd","LOCALS",1)
END

IF
  OR(2)
    See("cefald01")  // ~Faldorn~
    See("cefald02")  // ~Faldorn~
THEN
  RESPONSE #100
    Attack(LastSeenBy(Myself))
END

