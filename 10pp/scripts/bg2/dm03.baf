IF
  Clicked([ANYONE])
  Range(LastTrigger,10)
  GlobalTimerExpired("DM03Timer","GLOBAL")
  Global("DM03Active","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobalTimer("DM03Timer","GLOBAL",20)
    ForceSpell(LastTrigger,DM_COLD_PROTECTION)
    TriggerActivation("DM03",FALSE)
    SetGlobal("DM03Active","GLOBAL",1)
END

