IF
  OR(2)
    TimeGT(DUSK_END)
    TimeLT(5)
  See(NearestEnemyOf(Myself))
  Global("firstUse","LOCALS",0)
THEN
  RESPONSE #100
    ReallyForceSpell(NearestEnemyOf(Myself),BANSHEE_WAIL)
    SetGlobal("firstUse","LOCALS",1)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    AttackReevaluate(NearestEnemyOf(Myself),5)
END

