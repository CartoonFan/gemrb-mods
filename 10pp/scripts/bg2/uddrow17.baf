IF
  GlobalGT("PlayerDuelingLesaonar","GLOBAL",1)
  CombatCounter(0)
  Global("DuelOn","AR2202",1)
THEN
  RESPONSE #100
    SetGlobal("DuelOn","AR2202",0)
    SetGlobal("PlayerBarredFromDuel","GLOBAL",1)
    EscapeArea()
END

IF
  OR(2)
    StateCheck([PC],STATE_HELPLESS)
    StateCheck([PC],STATE_CONFUSED)
THEN
  RESPONSE #100
    MoveToObject([PC])
END

IF
  !See([GOODCUTOFF])
THEN
  RESPONSE #100
    RandomWalk()
END

