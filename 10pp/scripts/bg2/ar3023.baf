IF
  Global("EnteredArea","AR3023",0)
THEN
  RESPONSE #100
    SetGlobal("EnteredArea","AR3023",1)
    Explore()
END

IF
  Dead("FSSpir1")  // ~Spirit Warrior~
THEN
  RESPONSE #100
    Kill(Player1)
END

IF
  Global("FSSkelRaised","AR3023",1)
THEN
  RESPONSE #100
    SetGlobal("FSSkelRaised","AR3023",2)
    ActionOverride("FSSkel",PlayDeadInterruptible(1))
    CreateVisualEffectObject("SPFLESHS","FSSkel")
    Wait(1)
    ActionOverride("FSSpir1",StartDialogNoSet(Player1))
END

