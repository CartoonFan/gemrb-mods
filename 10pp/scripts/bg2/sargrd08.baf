IF
  See([GOODCUTOFF])
  Global("PrisonBreak","AR5007",0)
THEN
  RESPONSE #100
    SetGlobal("PrisonBreak","AR5007",1)
    PlaySound("ALARM1")
END

IF
  Global("PrisonBreak","AR5007",1)
  Allegiance(Myself,NEUTRAL)
THEN
  RESPONSE #100
    Enemy()
END

