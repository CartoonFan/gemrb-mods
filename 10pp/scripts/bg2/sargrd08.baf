IF
  Global("PrisonBreak","AR5007",0)
  Global("ClosingDoor","LOCALS",10)
  See("DOOR10")  // DOOR10
  OpenState("DOOR10",TRUE)  // DOOR10
THEN
  RESPONSE #20
    SetGlobal("ClosingDoor","LOCALS",0)
    CloseDoor("DOOR10")
  RESPONSE #10
    SetGlobal("ClosingDoor","LOCALS",0)
    CloseDoor("DOOR10")
    DisplayStringHead(Myself,71191)  // ~Is someone there?~
  RESPONSE #10
    SetGlobal("ClosingDoor","LOCALS",0)
    CloseDoor("DOOR10")
    DisplayStringHead(Myself,71192)  // ~These doors never seem to stay closed.~
  RESPONSE #10
    SetGlobal("ClosingDoor","LOCALS",0)
    CloseDoor("DOOR10")
    DisplayStringHead(Myself,71193)  // ~Must be the wind.~
END

