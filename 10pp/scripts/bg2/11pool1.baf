IF
  WalkedToTrigger([ANYONE])
THEN
  RESPONSE #100
    ClearAllActions()
    StartCutSceneMode()
    MoveViewObject(LastTrigger,INSTANT)
    SetGlobal("Spelunking","GLOBAL",0)
    DisplayStringNoName(Myself,72212)  // ~You dive through the tunnel and swim as fast as you can to the other end.~
    Wait(2)
    FadeToColor([30.0],0)
    Wait(2)
    ActionOverride(Player1,LeaveAreaLUAPanicEntry("AR6003","Entry4",[0.0],2))
    ActionOverride(Player1,LeaveAreaLUAEntry("AR6003","Entry4",[0.0],2))
    ActionOverride(Player2,LeaveAreaLUAEntry("AR6003","Entry4",[0.0],2))
    ActionOverride(Player3,LeaveAreaLUAEntry("AR6003","Entry4",[0.0],2))
    ActionOverride(Player4,LeaveAreaLUAEntry("AR6003","Entry4",[0.0],2))
    ActionOverride(Player5,LeaveAreaLUAEntry("AR6003","Entry4",[0.0],2))
    ActionOverride(Player6,LeaveAreaLUAEntry("AR6003","Entry4",[0.0],2))
    ActionOverride(Player1,Wait(2))
    ActionOverride(Player1,MultiPlayerSync())
    ActionOverride(Player1,FadeFromColor([30.0],0))
    ActionOverride(Player1,EndCutSceneMode())
END

IF
  WalkedToTrigger([ANYONE])
  GlobalTimerNotExpired("Pool1Timer","AR6011")
THEN
  RESPONSE #100
    ClearActions(LastTrigger)
END

