IF
  Detect([PC])
  Dead("ishclo01")  // ~Assassin ~
  Global("StartingDialogue","LOCALS",0)
THEN
  RESPONSE #100
    StartDialogNoSet([PC])
    SetGlobal("StartingDialogue","LOCALS",1)
END

IF
  Detect([PC])
  See([0.0.0.0.5])
THEN
  RESPONSE #100
    AttackReevaluate(LastSeenBy(Myself),15)
END

IF
  !Detect([PC])
  See([0.0.0.0.5])
THEN
  RESPONSE #100
    NoAction()
END

