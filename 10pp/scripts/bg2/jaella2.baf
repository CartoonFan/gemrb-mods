IF
  OR(2)
    GlobalTimerExpired("JanDidPlot","GLOBAL")
    GlobalTimerExpired("JaellaDead","GLOBAL")
THEN
  RESPONSE #100
    DestroySelf()
END

IF
  Global("JanLissaPlot","GLOBAL",14)
  See("Lissa")  // ~Lissa~
THEN
  RESPONSE #100
    MoveToObject("Lissa")
END

IF
  Global("JanLissaPlot","GLOBAL",14)
  !See("Lissa")  // ~Lissa~
THEN
  RESPONSE #100
    DestroySelf()
END

