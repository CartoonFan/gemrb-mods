IF
  !See([PC])
THEN
  RESPONSE #100
    MoveToPoint([681.2132])
END

IF
  Global("udKillGnomes","AR2100",1)
  Allegiance(Myself,NEUTRAL)
THEN
  RESPONSE #100
    Enemy()
END

IF
  Global("udleaveGnome","AR2100",1)
THEN
  RESPONSE #100
    EscapeArea()
END

IF
  See([PC])
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    EquipRanged()
    AttackReevaluate(LastSeenBy(Myself),15)
END

