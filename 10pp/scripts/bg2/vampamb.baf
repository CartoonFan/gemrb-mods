IF
  Global("CleanseVampire","AR1401",2)
  Global("AerieVampire","GLOBAL",3)
  Name("Aerie",Myself)
  AreaCheck("AR1401")
THEN
  RESPONSE #100
    SetGlobal("AerieVampire","GLOBAL",4)
    DialogueInterrupt(TRUE)
    ChangeAIScript("Aerie",OVERRIDE)
END

