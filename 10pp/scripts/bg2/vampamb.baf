    Global("AerieVampire","GLOBAL",1)
    Global("AnomenVampire","GLOBAL",1)
    Global("JaheiraVampire","GLOBAL",1)
    Global("ViconiaVampire","GLOBAL",1)
  Global("RomanceVampire","GLOBAL",1)
  Global("RomanceVampireTeleport","GLOBAL",0)
THEN
