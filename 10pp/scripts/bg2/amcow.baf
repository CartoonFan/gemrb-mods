IF
  General(Myself,ANIMAL)
  !Range("AMSHEP01",10)  // ~Cowherd~
  OnScreen(Myself)
THEN
  RESPONSE #100
    MoveToObject("AMSHEP01")
END

IF
  General(Myself,ANIMAL)
  Range("AMSHEP01",10)  // ~Cowherd~
  TimeGT(DAWN_START)
  TimeLT(DUSK_START)
  OnScreen(Myself)
THEN
  RESPONSE #100
    RandomWalk()
END

IF
  !General(Myself,ANIMAL)
  TimeGT(DAWN_START)
  TimeLT(DUSK_START)
