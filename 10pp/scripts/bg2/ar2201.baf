IF
  GlobalTimerExpired("udDemonSummon","GLOBAL")
  GlobalLT("udDrowPlot","GLOBAL",32)
  Global("summonTheDemon","AR2201",0)
THEN
  RESPONSE #100
    SetGlobal("doneRitual","AR2201",1)
    SetGlobal("summonTheDemon","AR2201",1)
    ScreenShake([50.50],5)
    MoveViewPoint([820.473],100)
    CreateVisualEffect("SPSUMGTE",[820.473])
    Wait(2)
    CreateVisualEffect("SPFIREPI",[820.473])
    Wait(2)
    CreateCreature("uddemon",[820.473],0)  // ~Lesser Demon Lord~
    ActionOverride("uddemon",Face(8))
END

