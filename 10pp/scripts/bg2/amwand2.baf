IF
  GlobalGT("Enclave","GLOBAL",1)
  Exists(Myself)
THEN
  RESPONSE #100
    DestroySelf()
END

IF
  TimeGT(DAWN_START)
  TimeLT(DUSK_START)
  !OnScreen(Myself)
  Global("wasnight","LOCALS",1)
THEN
  RESPONSE #100
    SetGlobal("notalk","LOCALS",0)
    SetGlobal("wasnight","LOCALS",0)
    Activate(Myself)
END

IF
  OR(2)
    TimeLT(DAWN_END)
    TimeGT(20)
  !OnScreen(Myself)
  Global("wasnight","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("notalk","LOCALS",1)
    Wait(2)
    SetGlobal("wasnight","LOCALS",1)
    Deactivate(Myself)
END

IF
  OR(2)
    TimeLT(DAWN_END)
    TimeGT(20)
  OnScreen(Myself)
  Global("wasnight","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("notalk","LOCALS",1)
    Wait(2)
    MoveToObject("DOOR03")
    Wait(1)
    SetGlobal("wasnight","LOCALS",1)
    Deactivate(Myself)
  RESPONSE #100
    SetGlobal("notalk","LOCALS",1)
    Wait(2)
    MoveToObject("DOOR04")
    Wait(1)
    SetGlobal("wasnight","LOCALS",1)
    Deactivate(Myself)
END

IF
  TimeGT(DAWN_START)
  TimeLT(DUSK_START)
  OnScreen(Myself)
THEN
  RESPONSE #100
    RandomWalk()
END

