IF
  Clicked([ANYONE])
  Range(LastTrigger,10)
  GlobalTimerExpired("DM04Timer","GLOBAL")
  Global("DM04Active","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobalTimer("DM04Timer","GLOBAL",20)
    ForceSpell(LastTrigger,DM_MAGIC_PROTECTION)
    TriggerActivation("DM04",FALSE)
    SetGlobal("DM04Active","GLOBAL",1)
END

