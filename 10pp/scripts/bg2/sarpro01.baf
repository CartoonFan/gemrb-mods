IF
  Detect([PC])
  Global("Display","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Display","LOCALS",1)
    DisplayStringHead(Myself,71203)  // ~Are you looking for a good time?~
END

IF
  Detect([PC])
  Global("Display","LOCALS",1)
  Delay(15)
THEN
  RESPONSE #100
    SetGlobal("Display","LOCALS",0)
END

