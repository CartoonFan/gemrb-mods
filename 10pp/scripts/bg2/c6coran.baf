IF
  AttackedBy([GOODCUTOFF],DEFAULT)
  Global("WerewolfHostile","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobal("WerewolfHostile","GLOBAL",1)
END

IF
  See([PC])
  Dead("c6lanf")  // ~Lanfear~
  Dead("c6safa")  // ~Safana~
  NumDeadGT("c6were2",5)
  Global("InitDialogue","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("InitDialogue","LOCALS",1)
    StartDialogNoSet([PC])
END

