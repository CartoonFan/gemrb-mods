IF
  OR(2)
    Global("moveMage4","AR2202",1)
    Global("PlayerBarredFromDuel","GLOBAL",1)
  Global("udOpenThePit","GLOBAL",0)
THEN
    ActionOverride("udchal",DestroySelf())
    Unlock("GATE2")
    OpenDoor("GATE2")
    SetGlobal("udOpenThePit","GLOBAL",1)
END

