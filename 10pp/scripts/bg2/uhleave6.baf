IF
  Global("CrowdDisperse","AR1100",1)
  Global("SpeechOver","LOCALS",0)
  AreaCheck("AR1100")
  See([PC])
THEN
  RESPONSE #100
    SetGlobal("SpeechOver","LOCALS",1)
    MoveToObjectNoInterrupt("TRAN1105")
    EscapeAreaMove("AR1105",717,502,10)
END

