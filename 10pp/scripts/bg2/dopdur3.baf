IF
  Die()
THEN
  RESPONSE #100
    SetGlobal("Dopdur3Move","GLOBAL",0)
END

IF
  Global("Dopdur3Move","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobal("Dopdur3Move","GLOBAL",1)
    DialogueInterrupt(FALSE)
    JumpToPoint([1673.880])
END

IF
  Global("Dopdur3Move","GLOBAL",1)
THEN
  RESPONSE #100
    SetGlobal("Dopdur3Move","GLOBAL",2)
    DialogueInterrupt(TRUE)
    StartDialogNoSet(Player1)
END

