IF
  GlobalGT("AnomenSaerk","GLOBAL",0)
THEN
  RESPONSE #100
    Kill(Myself)
END

IF
  GlobalTimerExpired("SaerkGuards","GLOBAL")
  Allegiance(Myself,NEUTRAL)
THEN
  RESPONSE #100
    Enemy()
END

IF
  GlobalGT("SaerkFightStart1","AR0505",0)
  AreaCheck("AR0505")
  Allegiance(Myself,NEUTRAL)
THEN
  RESPONSE #100
    Enemy()
END

IF
  Allegiance(Myself,ENEMY)
  !See([PC])
THEN
  RESPONSE #100
    MoveToObject([PC])
END

