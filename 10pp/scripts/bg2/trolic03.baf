IF
  OnCreation()
THEN
  RESPONSE #100
    StartTimer(77,6)
END

IF
  HPLT(Myself,12)
THEN
  RESPONSE #100
    StartTimer(77,6)
    ChangeAnimationNoEffect("TROLIC04")
END

IF
  TimerExpired(77)
THEN
  RESPONSE #100
    StartTimer(77,6)
END

IF
  !See([GOODCUTOFF])
  Allegiance(Myself,ENEMY)
  Range(Player1,50)
THEN
  RESPONSE #100
    MoveToObject(Player1)
END

