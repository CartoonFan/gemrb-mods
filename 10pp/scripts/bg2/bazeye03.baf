    ActionOverride("bazeye04",ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2))
    ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2)
    Wait(1)
    ActionOverride("bazeye04",ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2))
    ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2)
    SmallWait(5)
    ActionOverride("bazeye04",ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2))
    SmallWait(7)
    ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2)
    ActionOverride("bazeye04",ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2))
    SmallWait(7)
    ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2)
    ActionOverride("bazeye04",ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2))
    SmallWait(7)
    ReallyForceSpell("invisabo",BEHOLDER_BLAST_VISUAL2)
