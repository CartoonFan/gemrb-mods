IF
  See(NearestEnemyOf(Myself))
  Global("oozemephitcombat","LOCALS",0)
THEN
  RESPONSE #100
    ForceSpell(NearestEnemyOf(Myself),MEPHIT_STINKING_CLOUD)
    SetGlobal("oozemephitcombat","LOCALS",1)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    AttackReevaluate(NearestEnemyOf(Myself),30)
END

