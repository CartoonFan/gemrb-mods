IF
  TimeGT(5)
  TimeLT(DUSK_END)
  AreaType(OUTDOOR)
THEN
  RESPONSE #100
    EscapeArea()
END

IF
  Detect([PC])
  See([0.0.VAMPIRE])
THEN
  RESPONSE #100
    AttackReevaluate(LastSeenBy(Myself),15)
END

IF
  AttackedBy([GOODCUTOFF],DEFAULT)
  Global("PlayerAttackedAran","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobal("PlayerAttackedAran","GLOBAL",1)
END

