IF
  Die()
THEN
  RESPONSE #100
    ReallyForceSpell(Myself,FIREBALL_DEATH)
END

IF
  Dead("GOODDEAT")  // GOODDEAT
THEN
  RESPONSE #100
    ChangeAIScript("WTASIGHT",CLASS)
    ChangeAIScript("DEATHK",OVERRIDE)
END

IF
  See("GOODDEAT")  // GOODDEAT
THEN
  RESPONSE #100
    Attack("GOODDEAT")
END

