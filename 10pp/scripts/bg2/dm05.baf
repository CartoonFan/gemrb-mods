IF
  Clicked([ANYONE])
  Range(LastTrigger,10)
  GlobalTimerExpired("DM05Timer","GLOBAL")
  Global("DM05Active","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobalTimer("DM05Timer","GLOBAL",20)
    ForceSpell(LastTrigger,DM_SEE_INVISIBLE)
    TriggerActivation("DM05",FALSE)
    SetGlobal("DM05Active","GLOBAL",1)
END

