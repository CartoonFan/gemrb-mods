IF
  OR(2)
    AttackedBy([ANYONE],DEFAULT)
    HPLT(Myself,10)
  Global("ElhanEscape","LOCALS",0)
THEN
  RESPONSE #100
    ReallyForceSpell(Myself,DRYAD_TELEPORT)
    SetGlobal("ElhanEscape","LOCALS",1)
END

IF
  Global("TalkedToElhan","AR2800",1)
THEN
  RESPONSE #100
    StartCutSceneMode()
    StartCutScene("Cut66a")
END

