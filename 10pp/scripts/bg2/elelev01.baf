IF
  Clicked([ANYONE])
  Range(LastTrigger,10)
  Global("MachineOn","AR1505",0)
THEN
  RESPONSE #100
    SetGlobal("MachineOn","AR1505",1)
    CreateCreature("ELELEV01",[447.371],0)  // <Invalid Strref -1>
END

IF
  Clicked([ANYONE])
  Range(LastTrigger,10)
  Global("MachineOn","AR1505",1)
THEN
  RESPONSE #100
    ActionOverride("elelev01",StartDialogNoSet([PC]))
END

IF
  Clicked([ANYONE])
  !Range(LastTrigger,10)
THEN
  RESPONSE #100
    DisplayString(Myself,14702)  // ~You are too far away to use that.~
END

