IF
  Global("ELISIA_VANISH","GLOBAL",1)
THEN
  RESPONSE #100
    UseItem("WAND4CA",Myself)  // <Invalid Strref -1>
    Wait(3)
    DestroySelf()
END

IF
  AttackedBy([ANYTHING],DEFAULT)
THEN
  RESPONSE #100
    UseItem("WAND4CA",Myself)  // <Invalid Strref -1>
    SetGlobal("ELISIA_VANISH","GLOBAL",1)
    Wait(3)
    DestroySelf()
END

IF
  ActionListEmpty()
  !NearSavedLocation(0)
THEN
  RESPONSE #100
    ReturnToSavedLocation()
END

