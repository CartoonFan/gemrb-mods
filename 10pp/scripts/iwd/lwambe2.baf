IF
  !BitGlobal("LOCALS:CREATURE_STATE_VAR","",1,AND)
THEN
  RESPONSE #100
    BitGlobal("LOCALS:CREATURE_STATE_VAR","",1,OR)
    StartRandomTimer(24,5,10)
    Continue()
END

IF
  Global("MOVE_AMBERE","GLOBAL",2)
THEN
  RESPONSE #100
    DestroySelf()
END

IF
  CreatureHidden(Myself)
THEN
  RESPONSE #100
    NoAction()
END

IF
  GlobalLT("AMBERE_FAMILY","GLOBAL",2)
  !TimerActive(24)
THEN
  RESPONSE #50
    FloatMessage(Myself,24595)  // ~::Hic::~
    StartRandomTimer(24,5,10)
    Continue()
  RESPONSE #50
    FloatMessage(Myself,24596)  // ~"...tankard's almost done..."~
    StartRandomTimer(24,5,10)
    Continue()
  RESPONSE #50
    FloatMessage(Myself,24597)  // ~"...stupid Tybald..."~
    StartRandomTimer(24,5,10)
    Continue()
  RESPONSE #50
    FloatMessage(Myself,24598)  // ~"...fool can keep his filthy shop..."~
    StartRandomTimer(24,5,10)
    Continue()
  RESPONSE #250
    StartRandomTimer(24,5,10)
    Continue()
END

IF
  Global("MOVEMENT_LIST_0_LOC","LOCALS",0)
  ActionListEmpty()
  GlobalTimerExpired("MOVEMENT_LIST_0_TIMER","LOCALS")
THEN
  RESPONSE #100
    SetGlobalTimer("MOVEMENT_LIST_0_TIMER","LOCALS",3)
    SetGlobal("MOVEMENT_LIST_0_LOC","LOCALS",1)
END

