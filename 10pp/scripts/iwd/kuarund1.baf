IF
  HaveSpell(CLERIC_CREEPING_DOOM)
  GlobalLT("LOCAL_CAST_SPELL","LOCALS",1)
  See([PC])
THEN
  RESPONSE #100
    ForceSpell(LastSeenBy(Myself),CLERIC_CREEPING_DOOM)
    RemoveSpell(CLERIC_CREEPING_DOOM)
    Wait(2)
END

