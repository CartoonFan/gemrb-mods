IF
  !See([PC])
  LOS([PC],15)
  !TimerActive(25)
THEN
  RESPONSE #100
    StartTimer(25,7)
    ForceSpell([PC],CLERIC_INVISIBILITY_PURGE)
    Wait(2)
END

