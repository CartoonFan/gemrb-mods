IF
  Global("MOVEMENT_DEST","LOCALS",0)
  NearSavedLocation(1)
THEN
  RESPONSE #100
    ReturnToSavedLocation()
    SetGlobal("MOVEMENT_DEST","LOCALS",1)
  RESPONSE #100
    ReturnToSavedLocation()
    SetGlobal("MOVEMENT_DEST","LOCALS",2)
  RESPONSE #100
    ReturnToSavedLocation()
    SetGlobal("MOVEMENT_DEST","LOCALS",3)
END

IF
  !BitGlobal("LOCALS:CREATURE_STATE_VAR","",1,AND)
THEN
  RESPONSE #100
    BitGlobal("LOCALS:CREATURE_STATE_VAR","",1,OR)
    StartTimer(24,5)
    Continue()
END

IF
  Global("MOVEMENT_DEST","LOCALS",4)
  !TimerActive(24)
THEN
  RESPONSE #90
    SetGlobal("MOVEMENT_DEST","LOCALS",0)
    FloatMessage(Myself,16403)  // ~"Hehehe."~
    StartTimer(24,5)
    Continue()
  RESPONSE #90
    SetGlobal("MOVEMENT_DEST","LOCALS",0)
    FloatMessage(Myself,16404)  // ~"Is my ball here?"~
    StartTimer(24,5)
    Continue()
  RESPONSE #90
    SetGlobal("MOVEMENT_DEST","LOCALS",0)
    FloatMessage(Myself,16433)  // ~~
    StartTimer(24,5)
    Continue()
  RESPONSE #90
    SetGlobal("MOVEMENT_DEST","LOCALS",0)
    FloatMessage(Myself,16434)  // ~~
    StartTimer(24,5)
    Continue()
  RESPONSE #50
    SetGlobal("MOVEMENT_DEST","LOCALS",0)
    StartTimer(24,5)
    Continue()
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    RunAwayFromNoLeaveArea(LastSeenBy(Myself),45)
END

IF
  AttackedBy(NearestEnemyOf(Myself),DEFAULT)
THEN
  RESPONSE #100
    RunAwayFromNoLeaveArea(LastAttackerOf(Myself),45)
END

IF
  AttackedBy([GOODCUTOFF],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Continue()
END

