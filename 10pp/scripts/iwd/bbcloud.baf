IF
  Internal(Myself,4,2)
THEN
  RESPONSE #100
    ChangeCurrentScript("")
END

IF
  !TimerActive(4)
  See(NearestEnemyOf(Myself))
  Range(NearestEnemyOf(Myself),7)
  !CreatureHidden(Myself)
THEN
  RESPONSE #100
    IncrementInternal(Myself,4,1)
    StartTimer(4,21)
    ReallyForceSpell(Myself,INNATE_BOMBARDIER_BEETLE_CLOUD)
    Wait(2)
END

