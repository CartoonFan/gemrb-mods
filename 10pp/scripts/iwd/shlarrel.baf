    Wait(2)
  RESPONSE #100
    ForceSpell(LastSeenBy(Myself),WIZARD_DOMINATION)
    Wait(2)
  RESPONSE #100
    ForceSpell(LastSeenBy(Myself),WIZARD_CONE_OF_COLD)
    Wait(2)
  RESPONSE #100
    ForceSpell(LastSeenBy(Myself),WIZARD_CHAIN_LIGHTNING)
    Wait(2)
  RESPONSE #100
