IF
  !Allegiance(Myself,ENEMY)
  AttackedBy([GOODCUTOFF],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Continue()
END

IF
  HaveSpell(WIZARD_GLOBE_OF_INVULNERABILITY)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    ForceSpell(Myself,WIZARD_GLOBE_OF_INVULNERABILITY)
    RemoveSpell(WIZARD_GLOBE_OF_INVULNERABILITY)
    Wait(2)
END

IF
  HaveSpell(WIZARD_POWER_WORD_KILL)
  See(NearestEnemyOf(Myself))
  HPPercentLT(Myself,90)
  RandomNumLT(100,10)
THEN
  RESPONSE #100
    ForceSpell(LastSeenBy(Myself),WIZARD_POWER_WORD_KILL)
    RemoveSpell(WIZARD_POWER_WORD_KILL)
    Wait(2)
END

IF
  HaveSpell(WIZARD_CONJURE_EARTH_ELEMENTAL)
