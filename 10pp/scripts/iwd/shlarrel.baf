END

IF
  HaveSpell(WIZARD_DIRE_CHARM)
  See(NearestEnemyOf(Myself))
  !StateCheck(LastSeenBy(Myself),STATE_CHARMED)
  !TimerActive(25)
  RandomNumLT(100,10)
THEN
  RESPONSE #100
    StartTimer(25,30)
    ForceSpell(LastSeenBy(Myself),WIZARD_DIRE_CHARM)
    RemoveSpell(WIZARD_DIRE_CHARM)
    Wait(2)
END

