    ForceSpell(LastSeenBy(Myself),WIZARD_FIREBALL)
    Wait(2)
  RESPONSE #100
    ForceSpell(LastSeenBy(Myself),WIZARD_BELTYNS_BURNING_BLOOD)
    Wait(2)
  RESPONSE #100
