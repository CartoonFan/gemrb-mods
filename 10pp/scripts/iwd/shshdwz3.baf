IF
  !BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,AND)
  Or(2)
    See(NearestEnemyOf(Myself))
    See([ALLY])
THEN
  RESPONSE #100
    BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,OR)
    Continue()
END

IF
  !BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,AND)
  Or(2)
    AttackedBy(NearestEnemyOf(Myself),DEFAULT)
    AttackedBy([ALLY],DEFAULT)
THEN
  RESPONSE #100
    BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,OR)
    Continue()
END

IF
  BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,AND)
  !See(NearestEnemyOf(Myself))
  ActionListEmpty()
THEN
  RESPONSE #100
    MoveToObject([PC][0.0.10000.10000])
END

