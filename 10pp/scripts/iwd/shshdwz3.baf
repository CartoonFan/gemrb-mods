IF
  !BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,AND)
  GlobalGT("5104_CALL_WIZARD","GLOBAL",0)
THEN
  RESPONSE #100
    IncrementGlobal("5104_CALL_WIZARD","GLOBAL",-1)
    BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,OR)
    SetGlobal("PRE_BUFF","LOCALS",1)
    Continue()
END

IF
  Global("PRE_BUFF","LOCALS",1)
  BitGlobal("LOCALS:CREATURE_STATE_VAR","",512,AND)
THEN
  RESPONSE #100
    SetGlobal("PRE_BUFF","LOCALS",2)
    ReallyForceSpell(Myself,WIZARD_ARMOR)
    OpenDoor("AR5104DOOR6")
    Continue()
  RESPONSE #100
    SetGlobal("PRE_BUFF","LOCALS",2)
    ReallyForceSpell(Myself,WIZARD_SHIELD)
    OpenDoor("AR5104DOOR6")
    Continue()
END

IF
  HaveSpell(CLERIC_CURE_MODERATE_WOUNDS)
  !Dead("YSELF)")  // YSELF)
  HPPercentLT(Myself,60)
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    Spell(Myself,CLERIC_CURE_MODERATE_WOUNDS)
    Wait(2)
    Continue()
END

