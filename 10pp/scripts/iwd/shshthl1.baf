IF
  !BitGlobal("LOCALS:CREATURE_STATE_VAR","",32,AND)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    BitGlobal("LOCALS:CREATURE_STATE_VAR","",32,OR)
    Continue()
END

IF
  Global("SAW_ENEMY","LOCALS",0)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    SetGlobal("SAW_ENEMY","LOCALS",1)
    Continue()
END

