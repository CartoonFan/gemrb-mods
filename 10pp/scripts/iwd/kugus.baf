IF
  HaveSpell(CLERIC_CURSE)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    Spell(LastSeenBy(Myself),CLERIC_CURSE)
    Wait(2)
END

IF
  HaveSpell(CLERIC_MAGICAL_STONE)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    Spell(LastSeenBy(Myself),CLERIC_MAGICAL_STONE)
