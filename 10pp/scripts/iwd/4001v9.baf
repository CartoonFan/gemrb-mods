IF
  Die()
THEN
  RESPONSE #100
END

IF
  Global("LKingDead","GLOBAL",1)
  Global("Liz_King_Quest","GLOBAL",0)
  !See(NearestEnemyOf(Myself))
  See([PC])
  !Dead("yself)")  // yself)
THEN
  RESPONSE #100
    Dialogue([PC])
END

IF
  Global("LKingDead","GLOBAL",0)
  !TimerActive(1)
THEN
  RESPONSE #100
    FloatMessage(Myself,1821)  // ~"Shhh!"~
    StartRandomTimer(1,30,45)
  RESPONSE #100
    FloatMessage(Myself,1822)  // ~"I lost my dolly."~
    StartRandomTimer(1,30,45)
END

