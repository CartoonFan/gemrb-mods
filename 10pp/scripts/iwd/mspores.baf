IF
  Internal(Myself,4,5)
THEN
  RESPONSE #300
    ChangeCurrentScript("")
END

IF
  !TimerActive(4)
  See(NearestEnemyOf(Myself))
  !CreatureHidden(Myself)
THEN
  RESPONSE #300
    StartRandomTimer(4,7,21)
    Continue()
  RESPONSE #100
    IncrementInternal(Myself,4,1)
    StartRandomTimer(4,7,21)
    ReallyForceSpell(NearestEnemyOf(Myself),INNATE_MYCONID_SPORES)
    Wait(2)
END

