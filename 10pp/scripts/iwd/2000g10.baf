IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    Attack(NearestEnemyOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Help()
    Attack(LastAttackerOf(Myself))
END

IF
  Help([0.0.GOBLIN])
THEN
  RESPONSE #100
    Attack(LastAttackerOf(LastHelp))
END

IF
  !NearSavedLocation(7)
THEN
  RESPONSE #100
    ReturnToSavedLocation()
END

IF
  True()
THEN
  RESPONSE #100
    RandomWalk()
END

