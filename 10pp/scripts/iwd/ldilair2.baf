IF
  HaveSpell(WIZARD_LICH_TOUCH)
THEN
  RESPONSE #100
    Spell(NearestEnemyOf(Myself),WIZARD_LICH_TOUCH)
    Wait(2)
END

IF
  HaveSpell(WIZARD_HASTE)
  !StateCheck(NearestEnemyOf(Myself),STATE_HASTED)
THEN
  RESPONSE #100
    Spell(NearestEnemyOf(Myself),WIZARD_HASTE)
    Wait(2)
END

IF
  HaveSpell(WIZARD_TENSERS_TRANSFORMATION)
THEN
  RESPONSE #100
    Spell(NearestEnemyOf(Myself),WIZARD_TENSERS_TRANSFORMATION)
    Wait(2)
END

IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    AttackReevaluate(LastSeenBy(Myself),75)
END

IF
  ActionListEmpty()
THEN
  RESPONSE #100
    MoveToObject([PC][0.0.10000.10000])
END

