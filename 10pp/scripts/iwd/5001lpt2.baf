IF
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    Help()
    Attack(NearestEnemyOf(Myself))
END

IF
  AttackedBy([PC],DEFAULT)
THEN
  RESPONSE #100
    Help()
    Attack(LastAttackerOf(Myself))
END

IF
  Help([ANYONE])
THEN
  RESPONSE #100
    Attack(LastAttackerOf(LastHelp(Myself)))
END

IF
  Global("5001_Alert_2","AR5001",0)
  See([0.DEAD])
THEN
  RESPONSE #100
    SetGlobal("5001_Alert_2","AR5001",1)
END

IF
  Global("5001_Alert_2","AR5001",1)
THEN
  RESPONSE #100
    Attack([PC][0.0.10000.10000])
END

