IF
  See(NearestEnemyOf(Myself))
  Global("SPELL_STATE","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("SPELL_STATE","LOCALS",1)
    Spell(LastSeenBy(Myself),CLERIC_HOLD_PERSON)
    Wait(2)
  RESPONSE #100
    SetGlobal("SPELL_STATE","LOCALS",1)
    Spell(LastSeenBy(Myself),CLERIC_CHANT)
    Wait(2)
  RESPONSE #100
    SetGlobal("SPELL_STATE","LOCALS",1)
    Spell(LastSeenBy(Myself),CLERIC_SILENCE_15_RADIUS)
    Wait(2)
END

IF
  See(NearestEnemyOf(Myself))
  Global("SPELL_STATE","LOCALS",1)
THEN
  RESPONSE #100
    SetGlobal("SPELL_STATE","LOCALS",2)
    Spell(LastSeenBy(Myself),CLERIC_ENTANGLE)
    Wait(2)
  RESPONSE #100
    SetGlobal("SPELL_STATE","LOCALS",2)
    Spell(LastSeenBy(Myself),CLERIC_CURSE)
    Wait(2)
END

IF
  Global("SPELL_STATE","LOCALS",2)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    AttackReevaluate(LastSeenBy(Myself),75)
END

IF
  AttackedBy(NearestEnemyOf(Myself),DEFAULT)
THEN
  RESPONSE #100
    MoveToObject(LastAttackerOf(Myself))
END

IF
  !Allegiance(Myself,ENEMY)
  AttackedBy([GOODCUTOFF],DEFAULT)
THEN
  RESPONSE #100
    Enemy()
    Continue()
END

