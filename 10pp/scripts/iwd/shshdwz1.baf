IF
  Global("SPELL_PHASE","LOCALS",0)
  See(NearestEnemyOf(Myself))
THEN
  RESPONSE #100
    SetGlobal("SPELL_PHASE","LOCALS",1)
    Spell(NearestEnemyOf(Myself),WIZARD_SLOW)
    Wait(2)
    Continue()
  RESPONSE #100
    SetGlobal("SPELL_PHASE","LOCALS",1)
    Spell([ENEMY],WIZARD_HASTE)
    Wait(2)
    Continue()
END

