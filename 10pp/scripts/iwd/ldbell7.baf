IF
  Opened([PC])
  !BitGlobal("GLOBAL:LOWER_DORNS_DEEP_GAREA_VAR","",1,AND)
THEN
  RESPONSE #100
    BitGlobal("GLOBAL:LOWER_DORNS_DEEP_GAREA_VAR","",1,OR)
    SetGlobal("VOICE_NO_SPAWN","GLOBAL",1)
END

IF
  Unlocked([PC])
  !BitGlobal("GLOBAL:LOWER_DORNS_DEEP_GAREA_VAR","",1,AND)
THEN
  RESPONSE #100
    BitGlobal("GLOBAL:LOWER_DORNS_DEEP_GAREA_VAR","",1,OR)
    SetGlobal("VOICE_NO_SPAWN","GLOBAL",1)
END

