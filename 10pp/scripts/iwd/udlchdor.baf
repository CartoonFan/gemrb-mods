IF
  !Global("AR6006_VISITED","GLOBAL",0)
THEN
  RESPONSE #100
    OpenDoor(Myself)
    SetGlobal("KILL_LICH_MINIONS","MYAREA",1)
    SetGlobal("6005_NOXP_LICH_SPAWN","GLOBAL",0)
END

IF
  Opened([PC])
THEN
  RESPONSE #100
    SetGlobal("KILL_LICH_MINIONS","MYAREA",1)
    SetGlobal("6005_NOXP_LICH_SPAWN","GLOBAL",0)
END

IF
  Unlocked([PC])
THEN
  RESPONSE #100
    SetGlobal("KILL_LICH_MINIONS","MYAREA",1)
    SetGlobal("6005_NOXP_LICH_SPAWN","GLOBAL",0)
END

