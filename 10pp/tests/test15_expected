IF
  !Global("9300_ON_ISLAND","GLOBAL",0)
  Or(16)
    !OnIsland(Player1)
    !OnIsland(Player2)
    !OnIsland(Player3)
    !OnIsland(Player4)
    !OnIsland(Player5)
    !OnIsland(Player6)
    !OnIsland(Player7)
    !OnIsland(Player8)
    !Island(Player1)
    !Island(Player2)
    !Island(Player3)
    !Island(Player4)
    !Island(Player5)
    !Island(Player6)
    !Island(Player7)
    !Island(Player8)
  Or(2)
    !sland("Slayer1")
    !sland("Slayer2")
THEN
  RESPONSE #100
    SetGlobal("9300_ON_ISLAND","GLOBAL",0)
    Continue()
END

