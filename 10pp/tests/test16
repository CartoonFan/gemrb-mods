IF
  !Global("9300_ON_ISLAND","GLOBAL",0)
  OR(12)
    !OnIsland(Player1)
    !OnIsland(Player2)
    !OnIsland(Player3)
    !OnIsland(Player4)
    !OnIsland(Player5)
    !OnIsland(Player6)
    !Island(Player1)
    !Island(Player2)
    !Island(Player3)
    !Island(Player4)
    !Island(Player5)
    !Island(Player6)
  OR(13)
    !Onland(Player1)
    !Onland(Player2)
    !Onland(Player3)
    !Onland(Player4)
    !Onland(Player5)
    !Onland(Player6)
    Global(lalalala)
    !Isla(Player1)
    !Isla(Player2)
    !Isla(Player3)
    !Isla(Player4)
    !Isla(Player5)
    !Isla(Player6)
