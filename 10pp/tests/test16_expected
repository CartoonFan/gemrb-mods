    !Island(Player3)
    !Island(Player4)
    !Island(Player5)
    !Island(Player6)
    !Island(Player7)
    !Island(Player8)
  Or(17)
    !Onland(Player1)
    !Onland(Player2)
    !Onland(Player3)
    !Onland(Player4)
    !Onland(Player5)
    !Onland(Player6)
    !Onland(Player7)
